{
  "name": "vite-plugin-dev-qrcode",
  "version": "0.0.1",
  "description": "Vite plugin that displays QR codes for dev server URLs in terminal",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src",
    "prepare": "pnpm build",
    "example": "pnpm -C examples dev"
  },
  "keywords": [
    "vite",
    "plugin",
    "qrcode",
    "dev-server"
  ],
  "author": "maxinpeng",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sktm4a1/vite-plugin-dev-qrcode"
  },
  "bugs": {
    "url": "https://github.com/sktm4a1/vite-plugin-dev-qrcode/issues"
  },
  "homepage": "https://github.com/sktm4a1/vite-plugin-dev-qrcode#readme",
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "packageManager": "pnpm@7.16.1",
  "dependencies": {
    "qrcode-terminal": "^0.12.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/qrcode-terminal": "^0.12.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.5.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.0"
  }
}
